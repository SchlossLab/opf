read.lt <- function(d){
n <- as.numeric(d[1]); d <- d[-1]
m <- matrix(rep(NA, n*n), nrow=n)
groups <- d[1]; d <- d[-1]
for(i in 2:n){
groups <- c(groups, d[1]); d <- d[-1]
m[i,1:(i-1)] <- as.numeric(d[1:(i-1)])
d <- d[-c(1:(i-1))]
}
rownames(m) <- groups
colnames(m) <- groups
return(m)
}
